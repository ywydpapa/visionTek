/* 팝업 */
.dkPopup{position:absolute; text-align:left; z-index:999999; width:500px; left:50%; margin:120px 0 0 -250px;}
.dkPopImg{display:block;}
.dkPopClose{padding:5px 10px; background:#333; color:#fff; position:relative; font-size:12px;font-weight:normal;}
.dkPopClose input[type="checkbox"]{}
