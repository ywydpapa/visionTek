/* 텝*/ 
.tab {
	margin-bottom: 30px;
}
.tab ul {
	position:relative;
	text-align:center;
	overflow:hidden;
}
.tab ul:after {
	clear:both;
	content:"";
	display:block;
}
.tab ul li {
	display:inline-block; 
	height: 40px;
	line-height:38px;
	font-size:14px;
	margin-left:-5px;
	margin-bottom:5px;
	background:#fff;
}
.tab ul li a{
	display:block;
	padding: 0 18px;
	color:#333;
	border: 1px solid #d3d4d6;
	
}
.tab ul li a:hover{
	background:#333;
	border: 1px solid #333;
	color:#fff;
	text-decoration:none;
}
.tab ul li a.active{
	background:#333;
	border: 1px solid #333;
	color:#fff;
}

/* 기본테이블 */
.table-responsive {
	min-height: .01%;
	overflow-x: auto;
	margin-bottom:15px;
}
.tbl {
	width:100%;
	border-top: 1px solid #333333;
	margin-bottom:10px;
	box-sizing: border-box;
	 border-collapse: collapse;
}
.tbl tr th{
	padding: 14px 0;
	text-align:center;
	color:#282828;
	border-bottom: 1px solid #d3d3d3;
	border-left: 1px solid #d3d3d3;
	line-height: 150%;
	background:#f3f3f3;
}
.tbl tr th:first-child{
	border-left: none;
}

.tbl tr td{
	padding: 10px 5px 10px 7px;
	border-bottom: 1px solid #d3d3d3;
	border-left: 1px solid #d3d3d3;
	line-height: 150%;
	text-align:center;
	font-size:14px !important;
}
.tbl tr td *{
	font-size:14px !important;
}
.tbl tr td:first-child{
	border-left: none;
}
.tbl .bd_l {
	border-left:1px solid #d3d3d3 !important;
}
.tbl_tit {
	background:#f8f8f8;
	font-weight:bold;
	color:#282828;
}
.tbl_tit02 {
	color:#282828;
	font-weight:bold;
}
.tbl tr td .img_st {
	width:auto;
}


/* 버튼 */
.btnwr {
	text-align:center;
	margin-bottom: 30px;
	overflow:hidden;

}

.button,
.button *{
	display:inline-block; 
	overflow:visible; 
	border:0; 
	color:#fff;
	font-size:14px; 
	text-decoration:none; 
	white-space:nowrap; 
	text-align:center; 
	cursor:pointer; 
	cursor:hand;
}
.button.m {
	width: 90px; 
	height:40px;
	line-height:40px;	
	background:#333333;
	border: 2px solid #333333;
}

.button *:hover,
.button *:active,
.button *:focus{
	color:#333; 
	display:block;
	text-decoration:none; 
	background:#fff;
}

.str,
.str *{
	display:inline-block; 
	overflow:visible;
	border:0; 
	color:#fff; 
	font-size:14px; 
	text-decoration:none;
	white-space:nowrap; 
	text-align:center; 
	cursor:pointer; 
	cursor:hand; 
	margin-right:1px;
}
.str.m {
	width: 90px; 
	height:40px;
	line-height:40px;
	background:#c21c2c;	
	border: 2px solid #c21c2c;
}

.str *:hover,
.str *:active,
.str *:focus{
	color:#c21c2c; 
	text-decoration:none; 
	display:block;
	background:#fff;
}


