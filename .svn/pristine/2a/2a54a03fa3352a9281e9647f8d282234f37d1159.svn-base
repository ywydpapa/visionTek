	.dkPopClose{padding:5px 10px; background:#333; color:#fff; position:relative; font-size:12px;font-weight:normal;}
	.dkPopClose input[type="checkbox"]{}
	.dkPopClose button{background:none; border:0; font-size:12px; color:#fff; position:absolute; top:8px; right:5px; cursor:pointer;}

}

@media all and (max-width:640px){
