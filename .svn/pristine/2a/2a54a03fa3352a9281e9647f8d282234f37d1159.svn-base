/* sub */
/*************** //sub_visual *********/
#sub_container {
	clear:both; 
	width: 100%; 
	overflow:hidden;
}

#sub_visual { 
	width:100%; 
	height:360px; 
	text-align:center; 
	overflow:hidden; 
	position:relative; 
}
#sub_visual .viimg { 
	width:100%; 
	height:360px; 
}
@-webkit-keyframes visualZoom {
	0% {height:120%;}
	100% {height:100%;}
}
@-moz-keyframes visualZoom {
	0% {height:120%;}
	100% {height:100%;}
}
@-o-keyframes visualZoom {
	0% {height:120%;}
	100% {height:100%;}
}
@keyframes visualZoom {
	0% {height:120%;}
	100% {height:100%;}
}
.visualZoom {
	-webkit-animation-name: visualZoom;
	-moz-animation-name: visualZoom;
	-o-animation-name: visualZoom;
	animation-name: visualZoom;
	 -webkit-animation-duration: 2s;
    animation-duration: 2s;	
}
#sub_visual .sub_visual_txt {
	width:100%;
	margin: 0 auto;
	text-align:center;
	position:absolute; 
	top:43%;
	z-index: 10;
}
#sub_visual .sub_visual_txt .sub_vtxt_line {
	border-bottom: 1px solid #c21c2c;
	padding-bottom:5px;
	margin-bottom:22px;
}
#sub_visual .sub_visual_txt h2 {
	color:#fff;
	font-size:42px;
	font-weight:500;
}

.path {width: 100%;height: 50px;position:absolute;top:360px; background:#dfe2e4;clear:both;display:block;}
.path .depth { width: 1180px; margin: 0 auto;  line-height:50px;}
.path .depth > li { float: left; position:relative; display:inline-block; border-right:1px solid #fff; font-size:15px; width: 208px;}
.path .depth > li:first-child { border-left:1px solid #fff;}
.path .depth > li > a { color:#626262;  display:block; line-height:50px; font-weight:400; }
.path .depth > li.home{ float:left; width: 50px !important; height: 50px; background:url("../img/common/path_home.png")no-repeat 50% center;display:block;}
.path .depth > li.depth_sub > a { padding:0 0 0 20px;}
.path .depth > li > ul { display:none; position:absolute; top:50px; left:-2px; min-width:208px;  background:#fcfcfc;  /*box-shadow: 0px 1px 1px 1px #555;*/ border:#dedede solid 1px; z-index:100 }
.path .depth > li > ul > li { font-size:13px; border-bottom:#dedede solid 1px; /*border-bottom:#999 dashed 1px;*/ }
.path .depth > li.depth_sub > a:after { position: absolute; right: 23px; top: 23px; z-index: 5; display: block; height: 8px; width: 2px; background: #88898a; content: ""; transition: all 0.1s ease-out; }
.path .depth > li.depth_sub > a:after { transform: rotate(45deg); }
.path .depth > li.depth_sub > a:before { position: absolute; right: 25px; top: 26px; display: block; width: 8px; height: 2px; background: #88898a; content: "";  transition: all 0.1s ease-out; }
.path .depth > li.depth_sub > a:before { transform: rotate(225deg); }
.path .depth > li.depth_sub > a.open:after {transform: rotate(90deg); }
.path .depth > li.depth_sub > a.open:before {transform: rotate(180deg); }
.path .depth > li > ul > li:last-child { border:none; }
.path .depth > li > ul > li > a { display:block; padding:15px 20px ; line-height:120%; color:#555; }
.path .depth > li > ul > li > a:hover { background:#404850; color:#fff; box-shadow: 1px 1px 2px 0px #555; transition: all 0.5s ease-out; }

.path .depth > li.depth_sub2 { width: 238px;}
.path .depth > li.depth_sub2 > a:after {position: absolute; right: 23px; top: 23px; z-index: 5; display: block; height: 8px; width: 2px; background: #88898a; content: ""; transition: all 0.1s ease-out; }
.path .depth > li.depth_sub2 > a:after {transform: rotate(45deg); }
.path .depth > li.depth_sub2 > a:before {position: absolute; right: 25px; top: 26px; display: block; width: 8px; height: 2px; background: #88898a; content: "";  transition: all 0.1s ease-out; }
.path .depth > li.depth_sub2 > a:before {transform: rotate(225deg); }
.path .depth > li.depth_sub2 > a.open:after {transform: rotate(90deg); }
.path .depth > li.depth_sub2 > a.open:before {transform: rotate(180deg); }
.path .depth > li.depth_sub2 > a { padding:0 0 0 20px;}
.path .depth > li.depth_sub2 > ul { min-width:238px;}


