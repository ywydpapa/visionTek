@charset "utf-8";
/* CSS Document */
body {
	min-width:320px;
	height:100%;
}
#wrap {
	width: 100%;
	height:100%; 
	position:relative;
	font-family: 'Noto Sans KR', sans-serif;
}
.innerwrap {
	width: 1180px;	
	margin: 0 auto;
	overflow:hidden;
	position:relative;
}
#container {
	clear:both;
	width: 100%;
	overflow:hidden;
}
.bg{
	height:100%; 
	width:100%; 
	background-color:rgba(0,0,0,0.6); 
	position:absolute; 
	z-index:1001; 
	display:none;
}
 
/* 헤더 */
header {
	width: 100%;
	height: 80px;
	position:absolute;
	top: 0;
	z-index: 1020;
	/*background:#222222;
	background:rgba(34,34,34,0.8);*/
	background:rgba(0,0,0,0.5);
	border-bottom:1px solid rgba(255,255,255,0.3)
}
header section {
	width: 1180px;	
	margin: 0 auto;
	margin-top: 15px;
	overflow:hidden;
	text-align:right;
}
header h1.logo {
	width: 140px;
	height: 44px;
	float:left;
	margin-left: 10px;
}
header h1.logo a {
	display:inline-block;
}
.sidebar-toggle{
	display:none;
}

/* 메뉴 */
nav {
	width: auto;
	overflow:hidden;
	float:right;
	display:inline;
}
nav ul {
	margin: 0 auto;
}
nav ul li {
	height:65px;
	float:left;
	text-align:center;
	line-height: 61px;
	font-weight:normal;
	font-size: 14px; 
	color:#fff;
	font-size: 16px;
}
nav ul li a {
	padding: 0 22px;
	color:#fff;
	display:block;
	text-decoration:none;
}
nav ul li a:hover {
	color:#fff;
	text-decoration:none;
	border-bottom: 4px solid #c21c2c;
}
nav ul li .dth2 {
	clear:both;
	width:100%;
	min-height: 50px;
	line-height: 50px;
	z-index: 1030;
	position:absolute;
	top:81px;
	left: 0;
	background: rgba(0,0,0,0.5); 
	/*opacity: 0.85; 
	-ms-filter:progid:DXImageTransform.Microsoft.Alpha(Opacity= 85); /* // for IE8 
	filter: alpha(opacity= 85); /* //for IE5-7 */
	display:block;
	/*text-shadow:1px 1px 0px rgba(0,0,0,0.9);
	-webkit-text-shadow: 1px 1px 0px rgba(0,0,0,0.9); 
	-moz-text-shadow: 1px 1px 0px rgba(0,0,0,0.9);*/
}	
